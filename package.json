{
  "name": "ui-auto-test",
  "version": "1.0.0",
  "description": "基于Stagehand的UI自动测试工具，支持Excel测试用例解析",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "tsx src/index.ts",
    "start:compiled": "node dist/index.js",
    "test": "tsx src/index.ts",
    "test:local": "tsx src/index.ts --excel excel/test-cases-template.xlsx --headless false",
    "dev": "tsx watch src/index.ts",
    "llm:setup": "bash scripts/setup-ollama.sh",
    "debug": "DEBUG=true tsx src/index.ts --interactive --debug --headless false",
    "stop-recording": "touch records/.stop-recording"
  },
  "keywords": [
    "ui-testing",
    "automation",
    "stagehand",
    "excel"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/openai": "^3.0.19",
    "@browserbasehq/stagehand": "^3.0.8",
    "@playwright/test": "^1.58.0",
    "ai": "^6.0.49",
    "ai-sdk-ollama": "^3.3.0",
    "chalk": "^5.3.0",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "ollama-ai-provider-v2": "^3.0.2",
    "undici": "^7.19.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.21.0",
    "typescript": "^5.3.0"
  }
}
